<div id="<%= dom_id tenant %>">
  <p>
    <strong>First name:</strong>
    <%= tenant.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= tenant.last_name %>
  </p>

  <p>
    <strong>Email address:</strong>
    <%= tenant.email_address %>
  </p>

  <p>
    <strong>Phone number:</strong>
    <%= tenant.phone_number %>
  </p>

  <p>
    <strong>Property:</strong>
    <%= tenant.property_id %>
  </p>

  <p>
    <strong>Tenancy:</strong>
    <%= tenant.tenancy_id %>
  </p>

</div>